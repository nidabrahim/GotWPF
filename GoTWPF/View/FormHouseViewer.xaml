<Window x:Class="GoTWPF.View.FormHouseViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoTWPF.View"
        mc:Ignorable="d"
        Title="Form House" Height="300" Width="300">
    <StackPanel>
        <local:HouseView x:Name="ucHouse" ></local:HouseView>
        <DockPanel Margin="10">
            <Button Name="saveBtn" HorizontalAlignment="Left" Content="Save"  Click="Button_Click" />
            <Button   HorizontalAlignment="Right" Content="Cancel" Click="Button_Click_1" />
        </DockPanel>
        
    </StackPanel>
</Window>
